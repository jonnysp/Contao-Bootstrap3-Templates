<?php $this->block('body'); ?>



<div class="container" id="page">
 
 

			<?php $this->block('header'); ?>
		       <?php if ($this->header): ?>  
				<div class="row">
					<div class="col-sm-12">
	                <!--beginn section header-->
	                <?php echo $this->header; ?> 
	                <!--end section header-->
					</div>
				</div>
               <?php endif; ?>
    		<?php $this->endblock(); ?> 

 		
      <?php $this->block('container'); ?>   
          <div class="row" id="container">
              
     
                <?php $this->block('left'); ?>         
                    <?php if ($this->left): ?>
                        <div class="col-sm-3" id="left">
                           <!--beginn section left-->
                          <?php echo $this->left; ?>
                           <!--end section left-->
                         </div> 
                    <?php endif; ?>
                <?php $this->endblock(); ?>              
          

                <?php $this->block('main'); ?>
                   <?php if (!$this->left and !$this->right): ?>
                      <div class="col-sm-12">
                          <!--beginn section main-->
                          <?php echo $this->main; ?>
                        <!--end section main-->
                      </div> 
                  <?php elseif ($this->left and !$this->right) : ?>
                      <div class="col-sm-9">
                          <!--beginn section main-->
                          <?php echo $this->main; ?>
                        <!--end section main-->
                      </div> 
                  <?php elseif (($this->left and $this->right) or (!$this->left and $this->right)): ?>
                      <div class="col-sm-6">
                          <!--beginn section main-->
                          <?php echo $this->main; ?>
                        <!--end section main-->
                     </div>
                  <?php endif; ?>         
                <?php $this->endblock(); ?>
        
                 
     
              <?php $this->block('right'); ?>
                <?php if ($this->left and $this->right): ?>
                    <div class="col-sm-3">
                      <!--beginn section right-->
                        <?php echo $this->right; ?>
                      <!--end section right-->
                     </div>
                <?php elseif (!$this->left and $this->right): ?>
                    <div class="col-sm-6">
                       <!--beginn section right-->
                        <?php echo $this->right; ?>
                      <!--end section right-->
                    </div>
                <?php endif; ?> 
              <?php $this->endblock(); ?>
    

   
          </div>  
      <?php $this->endblock(); ?>  

 
 

      <?php $this->block('footer'); ?>
         <?php if ($this->footer): ?>
		    <div class="row">  
		          <div class="col-sm-12">
		            <!--beginn section footer-->
		            <?php echo $this->footer; ?>
		            <!--beginn section footer-->
		          </div>
		       </div>    
