  <form <?= $this->tableless ? '':'class="form-horizontal"' ?> <?php if ($this->action): ?> action="<?= $this->action; ?>"<?php endif; ?> id="<?= $this->formId; ?>" method="post" enctype="<?= $this->enctype; ?>">
