      <?php endif; ?>

      <?php if ($this->contact): ?>
        <fieldset>
          <legend><?= $this->contactDetails; ?></legend>
          <?= implode('', $this->contact); ?>
        </fieldset>
      <?php endif; ?>

      <?php if ($this->login): ?>
        <fieldset>
          <legend><?= $this->loginDetails; ?></legend>
          <?= implode('', $this->login); ?>
        </fieldset>
      <?php endif; ?>

      <?php if ($this->profile): ?>
        <fieldset>
          <legend><?= $this->profileDetails; ?></legend>
          <?= implode('', $this->profile); ?>
